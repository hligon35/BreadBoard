{
  "name": "bread-board",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "mobile",
    "web"
  ],
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.9.0",
    "@react-navigation/drawer": "^7.7.10",
    "@react-navigation/native": "^7.1.26",
    "@react-navigation/native-stack": "^7.9.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-web": "^0.21.0"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5"
  },
  "scripts": {
    "mobile": "npm --workspace mobile run start",
    "mobile:clear": "npm --workspace mobile run start:clear",
    "mobile:dev-client": "npm --workspace mobile run start:dev-client",
    "mobile:dev-client:clear": "npm --workspace mobile run start:dev-client:clear",
    "mobile:go": "npm --workspace mobile run start:go",
    "mobile:go:clear": "npm --workspace mobile run start:go:clear",
    "mobile:android": "npm --workspace mobile run android",
    "mobile:ios": "npm --workspace mobile run ios",
    "mobile:web": "npm --workspace mobile run web",
    "web": "npm --workspace web run dev",
    "web:build": "npm --workspace web run build",
    "web:preview": "npm --workspace web run preview",
    "typecheck": "npm --workspace web run typecheck && npm --workspace mobile run typecheck"
  }
}
